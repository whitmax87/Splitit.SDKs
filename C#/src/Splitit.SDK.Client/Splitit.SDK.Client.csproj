<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net46</TargetFrameworks>
    <Version>1.6.8</Version>
    <Authors>SplititSDK</Authors>
    <Title>Splitit C# SDK</Title>
    <PackageDescription>C# SDK for Splitit Web API.</PackageDescription>
    <RepositoryUrl>https://github.com/Splitit/Splitit.SDKs</RepositoryUrl>
    <PackageTags>Splitit</PackageTags>
